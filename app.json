{
  "expo": {
    "name": "Motoruum",
    "slug": "motoruum",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/Icon_new.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "deploymentTarget": "13.4"
          },
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "30.0.2",
            "kotlinVersion": "1.7.10"
          }
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "",
          "enableGooglePay": false
        }
      ],
      [
        "expo-document-picker",
        {
          "appleTeamId": "M34L8S85EP",
          "iCloudContainerEnvironment": "Production"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icon96x96.png",
          "color": "#ffffff",
          "mode": "production"
        }
      ],
      "expo-localization",
      "expo-font"
    ],
    "notification": {
      "icon": "./assets/Icon96x96_new.png",
      "color": "#ffffff",
      "iosDisplayInForeground": true,
      "androidMode": "default"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bitcode": "Debug",
      "usesAppleSignIn": true,
      "supportsTablet": true,
      "bundleIdentifier": "com.motoruum",
      "buildNumber": "1",
      "usesIcloudStorage": true,
      "infoPlist": {
        "NSUserTrackingUsageDescription": "Motoruum, kişiselleştirilmiş reklamlar sunmak için IDFA toplamayı amaçlıyor.",
        "NSCameraUsageDescription": "Motoruum, profil fotoğrafı ve İlan Görseli çekmek ve sunucuya yüklemek için kamerayı kullanır.",
        "NSPhotoLibraryUsageDescription": "Motoruum, profil fotoğrafı ve İlan Görseli seçmek ve yüklemek için fotoğraf kütüphanesine erişime ihtiyaç duyar.",
        "NSPhotoLibraryAddUsageDescription": "Motoruum, profil fotoğrafı ve İlan Görseli seçmek ve yüklemek için fotoğraf kütüphanesine erişime ihtiyaç duyar.",
        "NSLocationWhenInUseUsageDescription": "Motoruum, ilan bilgileri, İlan arama verileri ve Kullanıcı bilgileri için cihaz konumunu kullanır.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Motoruum, ilan bilgileri, İlan arama verileri ve Kullanıcı bilgileri için cihaz konumunu kullanır.",
        "NSLocationAlwaysUsageDescription": "Motoruum, ilan bilgileri, İlan arama verileri ve Kullanıcı bilgileri için cihaz konumunu kullanır.",
        "NSMicrophoneUsageDescription": "Motoruum, ürününün videosunu çekerken mikrofonunu kullanır ve ilan bilgileri için yükleme yapar.",
        "CFBundleAllowMixedLocalizations": true,
        "UIBackgroundModes": [
          "remote-notification"
        ]
      },
      "config": {
        "googleMapsApiKey": "h87nm7765gb87ni-ux2m6A",
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.motoruum",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon_new.png",
        "backgroundColor": "#fff"
      },
      "permissions": [
        "CAMERA",
        "CAMERA_ROLL",
        "MEDIA_LIBRARY",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_PHONE_STATE",
        "LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "RECEIVE_BOOT_COMPLETED",
        "android.permission.INTERNET",
        "POST_NOTIFICATIONS"
      ],
      "blockedPermissions": [
        "READ_CONTACTS",
        "RECORD_AUDIO"
      ],
      "versionCode": 41
    },
    "web": {
      "favicon": "./assets/Favicon_new.png"
    },
    "scheme": "motoruum",
    "extra": {
      "eas": {
        "projectId": "71a9f93b-9a2e-4467-b84a-a54e6b71695f"
      }
    }
  }
}
